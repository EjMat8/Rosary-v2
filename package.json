{
  "name": "rosary-v2",
  "version": "1.0.0",
  "description": "source for family home prayers",
  "default": "index.html",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "parcel index.html",
    "build": "parcel build index.html --dist-dir ./dist",
    "run-live": "npm-run-all --parallel start watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EjMat8/Rosary-v2.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EjMat8/Rosary-v2/issues"
  },
  "homepage": "https://github.com/EjMat8/Rosary-v2#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-beta.1",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "regenerator-runtime": "^0.13.7"
  }
}
